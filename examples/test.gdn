rt funzione(i32 value) {
    i32 i = 0;
    while (i < value * 2) {
        if (i < value) {
            printf("Il valore e' %d\n", i);
        } else {
            printf("Troppo grosso\n");
        }

        i = i + 1;
    }

    ret value + i;
}

rt main() {
    i32 x = funzione(10);
    printf("Result: %d\n", x);
}
